<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.example.kampregprogram.CreateTeamController">
    <children>
        <VBox alignment="TOP_CENTER" layoutX="190.0" layoutY="75.0" prefHeight="511.0" prefWidth="421.0">
            <children>
                <Label fx:id="titleLabel" alignment="CENTER" contentDisplay="CENTER" prefHeight="76.0" prefWidth="219.0" text="Opret Hold">
                    <font>
                        <Font size="24.0" />
                    </font>
                </Label>
                <Label text="Holdnavn" />
                <TextField fx:id="teamNameTextField">
                    <VBox.margin>
                        <Insets top="10.0" />
                    </VBox.margin>
                </TextField>
                <Label text="Antal Spillere">
                    <VBox.margin>
                        <Insets top="20.0" />
                    </VBox.margin>
                </Label>
                <TextField fx:id="numberOfPlayersTextField">
                    <VBox.margin>
                        <Insets top="10.0" />
                    </VBox.margin>
                </TextField>
                <Label text="Bynavn">
                    <VBox.margin>
                        <Insets top="20.0" />
                    </VBox.margin>
                </Label>
                <TextField fx:id="teamCityTextField">
                    <VBox.margin>
                        <Insets top="10.0" />
                    </VBox.margin>
                </TextField>
                <Button fx:id="createButton" mnemonicParsing="false" text="Opret" onAction="#handleCreateButtonClick">
                    <VBox.margin>
                        <Insets top="10.0" />
                    </VBox.margin>
                </Button>
            </children>
        </VBox>
    </children>
</AnchorPane>
